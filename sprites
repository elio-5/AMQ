                if is_within_range(self.pos.x, self.pos.y,37*TILESIZE,31*TILESIZE):
                    self.dice = True
                    #self.coll = True
                    self.sqone = False


            elif self.rand_num == 1 and self.arrowright:
                self.co = vec(29,30) * TILESIZE
