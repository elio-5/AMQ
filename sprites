    def collide_with_yellowspaces(self, dir):
        if dir == 'x' or dir == 'y':
            hits = pg.sprite.spritecollide(self, self.game.yellowspaces, False,collide_hit_rect)

    def collide_with_redspaces(self, dir):
        if dir == 'x' or dir == 'y':
            hits = pg.sprite.spritecollide(self, self.game.redspaces, False,collide_hit_rect)
            if hits and self.coin1 == 1:
                self.coins -= 3
                self.coin1 += 2
            elif not hits:
                self.coin1 = 1

    def collide_with_bossspaces(self, dir):
        if dir == 'x' or dir == 'y':
            hits = pg.sprite.spritecollide(self, self.game.bossspaces, False,collide_hit_rect)
            if hits and self.coin2 == 1:
                self.coins -= 5
                self.coin2 += 2
            elif not hits:
                self.coin2 = 1

    def collide_with_luckyspaces(self, dir):
        if dir == 'x' or dir == 'y':
            hits = pg.sprite.spritecollide(self, self.game.luckyspaces, False,collide_hit_rect)
            if hits and self.coin3 == 1:
                self.coins += 5
                self.coin3 += 2
            elif not hits:
                self.coin3 = 1



    def update(self):
        self.get_keys()
        self.rect = self.image.get_rect()
        self.player_movement()
        hits1 = pg.sprite.spritecollide(self, self.game.bluespaces, False, collide_hit_rect)
        if hits1 and self.coin == 1 and self.go:
            self.yum = True
            self.coins += 3
            self.coin += 2
            self.rollcount += 1
        elif not hits1:
            self.coin = 1
            #self.yum = False
        hits2 = pg.sprite.spritecollide(self, self.game.redspaces, False, collide_hit_rect)
        if hits2 and self.coin1 == 1 and self.go:
            self.yum = True
            self.coins -= 3
            self.coin1 += 2
            self.rollcount += 1
        elif not hits2:
            #self.yum = False
            self.coin1 = 1
        hits3 = pg.sprite.spritecollide(self, self.game.bossspaces, False, collide_hit_rect)
        if hits3 and self.coin2 == 1 and self.go:
            self.yum = True
            self.coins -= 5
            self.coin2 += 2
            self.rollcount += 1
        elif not hits3:
            #self.yum = False
            self.coin2 = 1
        hits4 = pg.sprite.spritecollide(self, self.game.luckyspaces, False, collide_hit_rect)
        if hits4 and self.coin3 == 1 and self.go:
            self.yum = True
            self.coins += 5
            self.coin3 += 2
            self.rollcount += 1
        elif not hits4:
            #self.yum = False
            self.coin3 = 1
        hits5 = pg.sprite.spritecollide(self, self.game.yellowspaces, False, collide_hit_rect)
        if hits5 and self.coin4 == 1 and self.go:
            self.yum = True
            self.coins += random.randint(4,9)
            self.coin4 += 2
            self.rollcount += 1
        elif not hits5 :
            #self.yum = False
            self.coin4 = 1

        if self.rand_num != -1 and self.co != (0,0):
            self.rot = (self.co - self.pos).angle_to(vec(1,0))
            self.acc = vec(PLAYER_SPEED,0).rotate(- self.rot)
            self.acc += self.vel * -5
            #print(self.acc)
        #elif self.rand_num  != -1 and self.co != (0,0):
            #self.rot = (self.co - self.pos).angle_to(vec(1, 0))
            #self.acc = vec(PLAYER_SPEED, 0).rotate(- self.rot)
            #self.acc += self.vel * -5
        else:
            self.rot = 0
            self.acc = vec(0,0)
            self.vel = vec(0,0)
        self.vel += self.acc * self.game.dt
        self.pos += self.vel * self.game.dt + 0.5 * self.acc * self.game.dt ** 2
        #print(self.vel)
        if all(element < 5 and element > -5 for element in self.vel):
            self.go = True
        else:
            self.go = False
        #if is_within_range(self.pos.x,self.pos.y):
            #self.dice = True
        self.hit_rect.centerx = self.pos.x
        self.hit_rect.centery = self.pos.y
        self.rect.center = self.hit_rect.center
        self.rect.center = self.pos
        #self.collide_with_bluespaces('x' or 'y')
        #self.collide_with_redspaces('x' or 'y')
        #self.collide_with_luckyspaces('x' or 'y')
        #self.collide_with_yellowspaces('x' or 'y')
        #self.collide_with_bossspaces('x' or 'y')

class bluespace(pg.sprite.Sprite):
    def __init__(self, game, x, y):
        self.groups = game.all_sprites, game.bluespaces
        pg.sprite.Sprite.__init__(self, self.groups)
        self.game = game
        self.image = pg.Surface((TILESIZE, TILESIZE))
        self.image.set_colorkey(BLUE )
        self.image.fill(BLUE)
        self.rect = self.image.get_rect()
        self.x = x
        self.y = y
        self.rect.x = x * TILESIZE
        self.rect.y = y * TILESIZE
class yellowspace(pg.sprite.Sprite):
    def __init__(self, game, x, y):
        self.groups = game.all_sprites, game.yellowspaces
